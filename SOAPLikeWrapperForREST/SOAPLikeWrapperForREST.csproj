<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>netstandard2.0</TargetFramework>
	<PackageId>Acumatica.$(AssemblyName)</PackageId>
	<Version>5.0.3-alpha</Version>
	<Company>Acumatica</Company>
	<Authors>Dmitrii-Naumov</Authors>
	<Product>Acumatica.$(AssemblyName)</Product>
	<Description>A C# wrapper allowing to write SOAP-like queries for Acuamtica REST API</Description>
	<Copyright>Acumatica</Copyright>
	<PackageProjectUrl>https://github.com/Acumatica/AcumaticaSOAPWrapperForREST</PackageProjectUrl>
	<RepositoryUrl>https://github.com/Acumatica/AcumaticaSOAPWrapperForREST</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageReleaseNotes>Removed misleading paramter from Get Method</PackageReleaseNotes>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<Title>A wrapper for Acumatica REST API Client making it look like deprecated SOAP API</Title>
	<PackageTags>Acumatica;REST;client;SOAP</PackageTags>
	<IncludeSymbols>True</IncludeSymbols>
	<SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Acumatica.RESTClient" Version="5.0.3-alpha" />
    <PackageReference Include="Acumatica.RESTClient.ContractBasedApi" Version="5.0.3-alpha" />
  </ItemGroup>

</Project>
